{
  "name": "@youspace/dashboard",
  "version": "1.0.0",
  "description": "dashboard you-space",
  "author": "henrique-manjaro <henriqueoliwork@gmail.com>",
  "scripts": {
    "serve:vue": "npm run serve --prefix=src/vue",
    "build": "npm run clean && npm run build --prefix src/vue && tsc -b",
    "build:dev": "npm run build:dev --prefix src/vue",
    "test": "npm run test:mocha && npm run test:vue",
    "test:mocha": "mocha -r ts-node/register ./src/__tests__/**/*",
    "test:vue": "npm run test --prefix=src/vue",
    "clean": "tsc -b --clean && rm -rf ./dist",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "index.ts"
  ],
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.4",
    "@types/node": "^12.12.22",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "bugs": {
    "url": "https://github.com/zzhenryquezz/you-space/issues"
  },
  "directories": {
    "dist": "dist",
    "__tests__": "__tests__"
  },
  "homepage": "https://github.com/zzhenryquezz/you-space#readme",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzhenryquezz/you-space.git"
  }
}
