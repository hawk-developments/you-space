{
  "name": "@youspace/dashboard",
  "version": "1.0.0",
  "description": "Dashboard files",
  "author": "Henrique de olivera",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:server": "ts-node --project lib/tsconfig.json lib/sandbox.ts",
    "dev:client": "npm run serve --prefix client",
    "build": "rm -rf dist && tsc -p lib",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
