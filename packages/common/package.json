  },
  "bugs": {
    "url": "https://github.com/zzhenryquezz/you-space/issues"
  },
  "dependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/pino": "^5.15.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "mocha": "^6.2.2",
    "pino": "^5.15.0",
    "pino-colada": "^1.5.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/sinon": "^7.5.1",
    "sinon": "^8.0.1"
  }
}
